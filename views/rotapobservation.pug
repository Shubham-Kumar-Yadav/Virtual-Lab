script.
  window.onload = function () {
  var dps=[];

  fetch('http://localhost:4000/store')
  .then(function (response) {
  return response.json();
  })
  .then(function (myjson) {
   for(let i=0;i<5;i++){
   dps.push({
   x: Number(myjson.CMFF[i]),
   y: Number(myjson.RAVP[i])
   });
   } 
  var chart = new CanvasJS.Chart("chartContainer", {
  animationEnabled: true,
  title: {
  text: "Graph Between Reciprocal of Average Particle Size Vs Cumulative Mass Fraction"
  },
  axisY: {
  title: "Reciprocal of Average Particle Size(1/mm)",
  suffix: "1/mm"

  },
  axisX: {
  title: "Cumulative Mass Fraction",
  suffix: ""

  },
  data: [{
  yValueFormatString: "",
  xValueFormatString: "",
  type: "splineArea",
  color: "rgba(54,158,173,.7)",
  markerSize: 5,
  dataPoints: dps
  
  }]
  });
  chart.render();
  })
  }
    
